{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/predict",
      "destination": "api/predict.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}