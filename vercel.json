{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/predict",
      "destination": "api/predict.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
